{
  "name": "vtk-dicom",
  "version": "0.8.12",
  "port-version": 3,
  "description": "DICOM for VTK",
  "homepage": "https://github.com/dgobbi/vtk-dicom",
  "license": "BSD-3-Clause",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vtk",
      "default-features": false
    },
    "zlib"
  ],
  "features": {
    "gdcm": {
      "description": "Use gdcm for decompressing DICOM files.",
      "dependencies": [
        "gdcm"
      ]
    }
  }
}
